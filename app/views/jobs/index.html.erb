<table class="table">
  <thead>
    <tr>
      <th scope="col">title</th>
      <th scope="col">short description</th>
      <th scope="col">salary</th>
    </tr>
  </thead>
  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.title %></td>
        <td><%= job.short_des %></td>
        <td><%= job.salary %></td>
      </tr>
    <% end %>
  </tbody>
</table>
